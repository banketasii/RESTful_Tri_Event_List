<h1>INDEX.EJS</h1>

<form action="/events/new" method="GET">
  <button type="submit">New Event</button>
</form>

<ul>
  <% for(let i = 0; i < events.length; i++) { %>
    <li>
      <span class="del-event-btn">
  <!--      <i class="fas fa-trash-alt"></i>-->
      </span> 
      <%= events[i]["date"].toLocaleDateString() %> :: <%= events[i]["type"] %> :: <%= events[i]["distance"] %> mi
  <!--    <img class="event-icon img-fluid" id="swimIcon">-->
      <a href="/events/<some_id>">View Event</a>
    </li>
  <% } %>
</ul>

